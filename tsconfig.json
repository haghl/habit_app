{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@navigation/*": ["src/navigation/*"],
      "@store/*": ["./src/store/*"],
      "@screen/*": ["./src/screen/*"],
      "@hooks/*": ["./src/hooks/*"]
    }
  }
}
